ock/drawSpike.js","original":{"line":32,"column":1}}],"sources":{"js/drawBlock/drawSpike.js":"export default function drawSpike(ctx, name, unit) {\r\n    ctx.save()\r\n    ctx.beginPath()\r\n    ctx.fillStyle='black'\r\n    ctx.strokeStyle='black'\r\n    if(name.split('_')[1] === 'bottom') {\r\n        ctx.moveTo(0,0)\r\n        ctx.lineTo(unit * 0.5, unit)\r\n        ctx.lineTo(unit, 0)\r\n        ctx.fill()\r\n    }\r\n    else if(name.split('_')[1] === 'top') {\r\n        ctx.moveTo(0, unit)\r\n        ctx.lineTo(unit * 0.5, 0)\r\n        ctx.lineTo(unit, unit)\r\n        ctx.fill()\r\n    }\r\n    else if(name.split('_')[1] === 'left') {\r\n        ctx.moveTo(0, 0)\r\n        ctx.lineTo(unit, unit*0.5)\r\n        ctx.lineTo(0, unit)\r\n        ctx.fill()\r\n    }\r\n    else if(name.split('_')[1] === 'right') {\r\n        ctx.moveTo(unit, 0)\r\n        ctx.lineTo(0, unit * 0.5)\r\n        ctx.lineTo(unit, unit)\r\n        ctx.fill()\r\n    }\r\n    ctx.closePath()\r\n    ctx.restore();\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"843a0aecc52afe3995745d4dc838cb92","cacheData":{"env":{}}}